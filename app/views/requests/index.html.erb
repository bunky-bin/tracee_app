<div class= "container"></div>
  <h1>All Requests</h1>
    <% @requests.each do |f| %>
      <div class="card" style= "background-color: lavender;height: 120px;width: 400px; margin: 10px">
        <p><%= f.commuter.first_name %> is going to meet <%= f.helper.first_name %> at <%= f.location%></p>
        <p>Request accepted at <%= f.accepted_time %></p>
      </div>
    <% end%>
</div>
